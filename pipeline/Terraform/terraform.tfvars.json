{
  "app_name": "kl",
  "s3bucket_layers": {
    "Klayers-defaultp38" : "klayers-default",
    "Klayers-devp38" : "klayers-bucket--devp38",
    "Klayers-prodp38": "klayers-bucket--prodp38",
    "Klayers-preprod": "klayers-preprod"
  },
  "aws_region": {
    "Klayers-defaultp38": "ap-southeast-1",
    "Klayers-devp38": "us-west-2",
    "Klayers-prodp38": "us-east-2",
    "Klayers-preprod": "ap-southeast-1"
  },
  "aws_profile": {
    "Klayers-defaultp38": "KlayersDefault",
    "Klayers-devp38": "KlayersDev",
    "Klayers-prodp38": "KlayersProdP38",
    "Klayers-preprod": "Klayerspreprod"
  },
  "api_domain_name":{
    "Klayers-defaultp38": "api-default.klayers.cloud",
    "Klayers-devp38": "api-dev.klayers.cloud",
    "Klayers-prodp38": "api.klayers.cloud",
    "Klayers-preprod": "api-preprod.klayers.cloud"
  },
  "github_repo": "git@github.com:keithrozario/Klayers.git",
  "s3bucket_config_parameter_name_suffix": "config_bucket/name",
  "github_role_name":{
    "Klayers-defaultp38": "github_role_default",
    "Klayers-devp38": "github_role_dev",
    "Klayers-prodp38": "github_role_prod",
    "Klayers-preprod": "github_role_preprod"
  },
  "cron_expression_invoke_pipelines":{
    "Klayers-defaultp38": "cron(0 0 5 31 2 ?)",
    "Klayers-devp38": "cron(0 0 5 31 2 ?)",
    "Klayers-prodp38": "cron(0 2 1 * ? *)",
    "Klayers-preprod": "cron(0 0 5 31 2 ?)"
  }
}
